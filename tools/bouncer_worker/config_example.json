{
	"logging": {
		"taskLogDir": "C:/sharedSpace/",
		"workerLogPath": "./bouncer_worker.log"
	},
	"rabbitmq":{
		"host": "amqp://username:password@localhost:5672",
		"worker_queue": "jobq",
		"callback_queue": "callbackq",
		"model_queue": "modelq",
		"task_prefetch": 4,
		"model_prefetch": 1,
		"sharedDir" : "C:/sharedSpace/"

	},
	"bouncer":{
		"path" : "C:/local/3drepobouncer/bin/repobouncerclient.exe",
		"envars": {
			"REPO_RVT_TEXTURES" : "C:/local/revit_materials/"
		}
	},
	"db": {
		"dbhost" :"localhost",
		"dbport": 27017,
		"username" : "username",
		"password" : "password"
	},
	"fs": {
		"path" : "~/",
		"level": 10
	},
	"defaultStorage": "db",
	"mongoimport":{
		"writeConcern": {
			"w": 1
		}
	},
	"unity":
	{
		"project": "C:\\unity.bat",
		"batPath": "buildBundle.bat"
	},
	"elastic": {
		"enabled"   : false,
		"cloudId"   : "cloudhost:xxx",
		"cloudAuth" : "username:password"
	},
	"processMonitoring" : {
		"enabled" : false,
		"memoryIntervalMS" : 10
	}
}
