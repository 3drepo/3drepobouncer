{
	"logLocation": "log.log",
	"rabbitmq":{
		"host": "amqp://username:password@localhost:5672",
		"worker_queue": "jobq",
		"callback_queue": "callbackq",
		"model_queue": "modelq",
		"task_prefetch": 4,
		"model_prefetch": 1
	},
	"bouncer":{
		"path" : "C:/local/3drepobouncer/bin/repobouncerclient.exe",
		"log_dir" : "/var/log/3drepo/bouncer/",
		"envars": {
			"REPO_RVT_TEXTURES" : "C:/local/revit_materials/"
		}
	},
	"db": {
		"dbhost" :"localhost",
		"dbport": 27017,
		"username" : "username",
		"password" : "password"
	},
	"aws": {
		"bucket_name" : "3drepo-sandbox",
		"bucket_region" : "eu-west-2"
	},
	"fs": {
		"path" : "~/",
		"level": 10
	},
	"defaultStorage": "db",
	"mongoimport":{
		"writeConcern": {
			"w": 1
		}
	},
	"unity":
	{
		"project": "C:\\unity.bat",
		"batPath": "buildBundle.bat"
	}

}
